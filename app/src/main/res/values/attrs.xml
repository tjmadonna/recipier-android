<resources>

    <declare-styleable name="AspectRatioView">
        <attr name="aspectRatio" format="float" />
    </declare-styleable>

    <declare-styleable name="KeywordsLinearLayout">
        <attr name="editable" format="boolean" />
        <attr name="keywordTextColor" format="color"/>
        <attr name="addKeywordTextColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="ForegroundRelativeLayout">
        <attr name="android:foreground" />
        <attr name="foregroundInsidePadding" format="boolean"/>
        <attr name="android:foregroundGravity" />
    </declare-styleable>

</resources>